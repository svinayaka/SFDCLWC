    <rules>
        <fullName>GE DS Visa Status update to Expired</fullName>
        <actions>
            <name>GE_DS_Visa_Status_update_to_Expired</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_OG_GS_Visa__c.GE_OG_GS_Visa_Expiry_Date__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </criteriaItems>
        <description>R-28979</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>GE_OG_GS_Set_Visa_Status_to_Expired</fullName>
        <active>true</active>
        <criteriaItems>
            <field>GE_OG_GS_Visa__c.GE_OG_GS_Visa_Expiry_Date__c</field>
            <operation>lessThan</operation>
            <value>TODAY</value>
        </criteriaItems>
        <criteriaItems>
            <field>GE_OG_GS_Visa__c.GE_OG_GS_Visa_Status__c</field>
            <operation>notEqual</operation>
            <value>Replaced</value>
        </criteriaItems>
        <description>Set the Visa Status to Expired with time based workflow after expiry date if not already set to replaced</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>GE_OG_GS_Visa_Visa_Status_Expired</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>GE_OG_GS_Visa__c.GE_OG_GS_Visa_Expiry_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>GE_OG_GS_Visa Expired</fullName>
        <active>true</active>
        <description>AGrimes 1/8 R-19369</description>
        <formula>AND(  GE_OG_GS_Technician__r.SVMXC__Active__c = TRUE,  ISPICKVAL(GE_OG_GS_Visa_Status__c , &quot;Active&quot;),  GE_OG_GS_Visa_Expiry_Date__c &gt; Today()  )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
