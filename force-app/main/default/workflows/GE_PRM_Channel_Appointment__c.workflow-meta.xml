    <rules>
        <fullName>Email notification for TMS MENAT Channel Appointments</fullName>
        <actions>
            <name>Email_notification_for_TMS_MENAT_Channel_Appointment</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Tier_2_P_L__c</field>
            <operation>equals</operation>
            <value>TMS</value>
        </criteriaItems>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_O_G_Region__c</field>
            <operation>equals</operation>
            <value>Middle East / North Africa</value>
        </criteriaItems>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Total_Count_CommerApp_Approvals__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <description>FYI Email notification to Valter Quercioli when a Channel Appointment is created for TMS tier and for region MENAT</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE PRM Appointment Rejected</fullName>
