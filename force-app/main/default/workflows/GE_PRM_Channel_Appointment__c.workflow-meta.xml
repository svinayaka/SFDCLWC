        <actions>
            <name>GE_PRM_Commercial_Line_approved</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>GE_PRM_CommercailApp_date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_Update_the_Status_Draft</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(GE_PRM_Approved_CommercialApp_Approvals__c!=0,GE_PRM_Total_Count_CommerApp_Approvals__c!=0,GE_PRM_Approved_CommercialApp_Approvals__c == GE_PRM_Total_Count_CommerApp_Approvals__c, GE_PRM_DOA_generated__c!=true, GE_PRM_IsApproved__c ==false )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE PRM CommerApp after approved for additional CL</fullName>
        <actions>
            <name>GE_PRM_Commercial_Line_approved</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>GE_PRM_UpdateEnableCommButton</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_Update_the_Status_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_additionalCommercailApp_date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_isCommercialAffectedFalse</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_isStatusChangedTrue</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(GE_PRM_Approved_CommercialApp_Approvals__c!=0,GE_PRM_Total_Count_CommerApp_Approvals__c!=0,GE_PRM_Approved_CommercialApp_Approvals__c == GE_PRM_Total_Count_CommerApp_Approvals__c, GE_PRM_DOA_generated__c==true, GE_PRM_IsApproved__c ==true, GE_PRM_IsstatusChanged__c ==true, GE_PRM_CommercialAffected__c ==true, GE_PRM_ComplainceAffected__c ==false, GE_PRM_WillUpdateCommercial__c ==false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE PRM CommerApp approved for additional CL</fullName>
        <actions>
            <name>GE_PRM_AdtinalCommercial_Line_approved</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>GE_PRM_Updatestatus_ADDCL_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>GE_PRM_additionalCommercailApp_date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(GE_PRM_Approved_CommercialApp_Approvals__c!=0,GE_PRM_Total_Count_CommerApp_Approvals__c!=0,GE_PRM_Approved_CommercialApp_Approvals__c == GE_PRM_Total_Count_CommerApp_Approvals__c, GE_PRM_DOA_generated__c==true, GE_PRM_IsApproved__c ==false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_ChApp_NR</fullName>
        <actions>
            <name>GE_PRM_ChApp_NR</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(GE_PRM_Total_no_of_commercial_lines__c = GE_PRM_NR_Approved_commercial_lines__c,GE_PRM_Total_no_of_commercial_lines__c &lt;&gt; 0 ,GE_PRM_NR_Approved_commercial_lines__c&lt;&gt; 0)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_ChApp_Terminated</fullName>
        <actions>
            <name>GE_PRM_ChApp_Terminated</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(GE_PRM_Total_no_of_commercial_lines__c  =  GE_PRM_Terminated_commercial_lines__c  +  GE_PRM_NR_Approved_commercial_lines__c ,GE_PRM_Total_no_of_commercial_lines__c  &lt;&gt; 0 ,GE_PRM_Terminated_commercial_lines__c  &gt;=  1)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_DOA_Generated_Date</fullName>
        <actions>
            <name>GE_PRM_Update_DOA_Generated_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Total_Count_of_CA_Approvals__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <description>This Workflow Rule will update DOA Generated Date when DOA is Generated on Channel appointment object</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_Deloitte order date</fullName>
        <actions>
            <name>GE_PRM_Deloitte_order_date</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Deloitte order date</description>
        <formula>AND( GE_PRM_Deloitte_report_ordered__c, NOT( ISNULL(GE_PRM_Deloitte_report_order_date__c)))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_Government certificate ticked</fullName>
        <actions>
            <name>GE_PRM_Government_certificate_ticked</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_KYC_DD_Submitted_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Risk score saved + Government certificate ticked</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_Master Appointment %23 generated</fullName>
        <actions>
            <name>GE_PRM_Master_Appointment_generated</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Master Appointment # generated</description>
        <formula>true</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_Was_Ever_High</fullName>
        <actions>
            <name>GE_PRM_Was_Ever_High_Flag_Checked</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Risk_rating__c</field>
            <operation>equals</operation>
            <value>High</value>
        </criteriaItems>
        <description>If the Risk Rating is &quot;High&quot; &quot;Was Ever High&quot; flag will be checked.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Master Appointment Expiry</fullName>
        <active>false</active>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Expirydate1__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>GE_PRM_Channel_Appointment__c.GE_PRM_Current_Status__c</field>
            <operation>notEqual</operation>
            <value>Abandoned</value>
        </criteriaItems>
        <description>Master Appointment Expiry</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Master_Appointment_90_days_expiry</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>GE_PRM_Channel_Appointment__c.GE_PRM_Expirydate__c</offsetFromField>
            <timeLength>-90</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Master_Appointment_30_days_expiry</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>GE_PRM_Channel_Appointment__c.GE_PRM_Expirydate__c</offsetFromField>
            <timeLength>-30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Master_Appointment_60_days_expiry</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>GE_PRM_Channel_Appointment__c.GE_PRM_Expirydate__c</offsetFromField>
            <timeLength>-60</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
</Workflow>
