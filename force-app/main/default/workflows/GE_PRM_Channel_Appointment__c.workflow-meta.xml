        <active>true</active>
        <description>Created Workflow When Appointment Rejected.</description>
        <formula>And(GE_PRM_Pending_CA_Approvals__c=0, GE_PRM_Total_Count_of_CA_Approvals__c &gt; 0, GE_PRM_Need_Info_ChannelApp_Approvals__c = 0, GE_PRM_Rejected_ChannelApp_Approvals__c &gt; 0)</formula>
