    <rules>
        <fullName>BHGE_GS_InboundComplete_Notify</fullName>
        <actions>
            <name>BHGE_GS_Complete_Inbound_Notify</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_OG_GS_Mobilization__c.GE_OG_GS_Mobilization_Status__c</field>
            <operation>equals</operation>
            <value>COMPLETED</value>
        </criteriaItems>
        <criteriaItems>
            <field>GE_OG_GS_Mobilization__c.Record_Type_Name__c</field>
            <operation>equals</operation>
            <value>TPS Shipping</value>
        </criteriaItems>
        <description>R-34013</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>BHGE_GS_InboundShipping_Alert</fullName>
        <actions>
            <name>BHGE_GS_MOBEQ_Arrival_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_OG_GS_Mobilization__c.GE_OG_GS_Mobilization_Status__c</field>
            <operation>equals</operation>
            <value>AT SITE</value>
        </criteriaItems>
        <criteriaItems>
            <field>GE_OG_GS_Mobilization__c.Record_Type_Name__c</field>
            <operation>equals</operation>
            <value>TPS Shipping</value>
        </criteriaItems>
        <description>R-33995</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>BHGE_GS_MOBEQInbound_Alert</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>GE_OG_GS_Mobilization__c.BHGE_GS_Work_Order_CustomerEndDate__c</offsetFromField>
            <timeLength>-5</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
