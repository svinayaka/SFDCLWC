    <fieldUpdates>
        <fullName>GE_PRM_Agreement_Expiration_Calculation</fullName>
        <field>GE_PRM_Agreement_expiration__c</field>
        <formula>DATE(CASE( GE_PRM_Agreement_term__c , &apos;1 Year&apos;, 1, &apos;2 Years&apos;, 2, &apos;3 Years&apos;, 3, &apos;4 Years&apos;, 4, &apos;5 Years&apos;, 5, &apos;6 Years&apos;, 6, &apos;7 Years&apos;, 7, &apos;8 Years&apos;, 8, &apos;9 Years&apos;, 9, &apos;10 Years&apos;, 10, &apos;11 Years&apos;, 11, &apos;12 Years&apos;, 12, &apos;13 Years&apos;, 13, &apos;14 Years&apos;, 14, &apos;15 Years&apos;, 15, &apos;16 Years&apos;, 16, 0) + YEAR(GE_PRM_Agreement_effective_date__c), MONTH(GE_PRM_Agreement_effective_date__c), DAY(GE_PRM_Agreement_effective_date__c ) ) -1</formula>
        <name>GE_PRM_Agreement_Expiration_Calculation</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
