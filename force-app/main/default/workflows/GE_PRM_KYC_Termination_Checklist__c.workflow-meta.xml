        </actions>
        <actions>
            <name>KYC_Sent_Back_Interim_Time_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>KYC_Sent_Back_Time_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>KYC_Submitted_Interim_Time_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>KYC_Submitted_Time_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Final_KYC_Submitted_Timestamp</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Initial_KYC_Submitted_Timestamp</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_the_KYC_Sent_Back_TimeStamp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This workflow is used to update the total sent back and submitted time.</description>
        <formula>ISCHANGED(GE_HQ_Status__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
