    <fieldUpdates>
        <fullName>GE_OG_Update_Fin_Status_NotStarted</fullName>
        <field>GE_OG_Fin_Status__c</field>
        <literalValue>Not Started</literalValue>
        <name>GE_OG_Update_Fin_Status_NotStarted</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>GE_OG_Update_Final_KYC_Sent_Back</fullName>
        <field>GE_OG_Final_KYC_Sent_Back_TimeStamp__c</field>
        <formula>IF(AND(GE_OG_KYC_Old_value__c = &apos;Manual Due Diligence Submitted&apos;,
GE_OG_KYC_New_Value__c =&apos;Manual Due Diligence Sent back&apos;,
GE_OG_KYC_SentBckCount__c &gt;=1)
,NOW(), PRIORVALUE(GE_OG_Final_KYC_Sent_Back_TimeStamp__c) )</formula>
        <name>Update Final KYC Sent Back TimeStamp</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Inbound_Response_Update</fullName>
        <field>KYC_Inbound_Response__c</field>
        <formula>NOW()</formula>
        <name>KYC Inbound Response Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Outbound_Response_Update</fullName>
        <field>KYC_Outbound_Response__c</field>
        <formula>NOW()</formula>
        <name>KYC Outbound Response Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Sent_Back_Interim_Time_Update</fullName>
        <field>GE_OG_KYC_SentBack_Interim_Time__c</field>
        <formula>IF(TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Sent back&apos;,
NOW(), PRIORVALUE( GE_OG_KYC_SentBack_Interim_Time__c ) )</formula>
        <name>KYC Sent Back Interim Time Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Sent_Back_Time_Update</fullName>
        <field>GE_OG_KYC_Sent_Back_Time__c</field>
        <formula>IF(AND(TEXT(PRIORVALUE(GE_HQ_Status__c))= &apos;Manual Due Diligence Sent back&apos;,
TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Submitted&apos;,
GE_OG_KYC_SentBckCount__c &gt;=1), GE_OG_KYC_Sent_Back_Time__c + 
(( GE_OG_Final_KYC_Sent_Back_TimeStamp__c -  GE_OG_KYC_SentBack_Interim_Time__c )*24),
PRIORVALUE( GE_OG_KYC_Sent_Back_Time__c ) )</formula>
        <name>KYC Sent Back Time Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Submitted_Interim_Time_Update</fullName>
        <field>GE_OG_KYC_Submitted_Interim_Time__c</field>
        <formula>IF(TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Submitted&apos;,
NOW(), PRIORVALUE( GE_OG_KYC_Submitted_Interim_Time__c ) )</formula>
        <name>KYC Submitted Interim Time Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>KYC_Submitted_Time_Update</fullName>
        <field>GE_OG_KYC_SubmittedTime__c</field>
        <formula>IF(AND(TEXT(PRIORVALUE(GE_HQ_Status__c)) = &apos;Manual Due Diligence Submitted&apos;,OR(TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Sent back&apos;,TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Approved&apos;,TEXT(GE_HQ_Status__c)=&apos;Manual Due Diligence Rejected&apos;),
 GE_OG_KYC_Sub_Counter__c &gt;=1), GE_OG_KYC_SubmittedTime__c + (( GE_OG_Final_KYC_Sub_Timestamp__c -  GE_OG_KYC_Submitted_Interim_Time__c )*24),
PRIORVALUE(  GE_OG_KYC_SubmittedTime__c  ) )</formula>
        <name>KYC Submitted Time Update</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_Final_KYC_Submitted_Timestamp</fullName>
        <field>GE_OG_Final_KYC_Sub_Timestamp__c</field>
        <formula>IF(AND(OR(GE_OG_KYC_Old_value__c=&quot;Manual Due Diligence Rejected&quot;,GE_OG_KYC_Old_value__c=&quot;Manual Due Diligence Sent back&quot;,GE_OG_KYC_Old_value__c=&quot;Manual Due Diligence Reviewed&quot;),OR(GE_OG_KYC_New_Value__c = &quot;Manual Due Diligence Submitted&quot;,GE_OG_KYC_New_Value__c = &quot;Manual Due Diligence Approved&quot;,GE_OG_KYC_New_Value__c = &quot;Manual Due Diligence Rejected&quot;),GE_OG_KYC_Sub_Counter__c &gt;=1),NOW(), PRIORVALUE(GE_OG_Final_KYC_Sub_Timestamp__c ))</formula>
        <name>Update Final KYC Submitted Timestamp</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_Initial_KYC_Submitted_Timestamp</fullName>
        <field>GE_OG_Initial_KYC_Sub_Timestamp__c</field>
        <formula>IF(AND( GE_OG_KYC_Old_value__c = &apos;Pending User Submission&apos;, GE_OG_KYC_New_Value__c = &apos;Manual Due Diligence Submitted&apos;),NOW(),PRIORVALUE( GE_OG_Initial_KYC_Sub_Timestamp__c))</formula>
        <name>Update Initial KYC Submitted Timestamp</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_the_KYC_Age_in_hrs</fullName>
        <description>Update the KYC Cycle time for Each Status changed and also the KYC age in hours</description>
        <field>GE_OG_KYC_Age_in_hrs__c</field>
        <formula>IF( ISNULL( GE_OG_KYC_Age_in_hrs__c ) ,((Now()- PRIORVALUE( LastModifiedDate ) )*24),(((Now()- PRIORVALUE( LastModifiedDate ) )*24)+ PRIORVALUE(GE_OG_KYC_Age_in_hrs__c) ))</formula>
        <name>Update the KYC Age (in hrs)</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_the_KYC_Sent_Back_TimeStamp</fullName>
        <field>GE_OG_KYC_Sent_Back_TimStamp__c</field>
        <formula>IF(AND(GE_OG_KYC_Old_value__c = &apos;Manual Due Diligence Submitted&apos;,
GE_OG_KYC_New_Value__c =&apos;Manual Due Diligence Sent back&apos;,
 GE_OG_KYC_Sub_Counter__c = 1)
,NOW(), PRIORVALUE(GE_OG_KYC_Sent_Back_TimStamp__c))</formula>
        <name>Update Initial KYC Sent Back TimeStamp</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
