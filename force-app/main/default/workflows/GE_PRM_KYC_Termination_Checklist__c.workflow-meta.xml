    </rules>
    <rules>
        <fullName>GE_PRM_KYC_Complete_Rule</fullName>
        <actions>
            <name>GE_PRM_KYC_Complete_To_Channel_Manager</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>GE_PRM_KYC_Termination_Checklist__c.GE_PRM_Status__c</field>
            <operation>equals</operation>
            <value>KYC Complete</value>
        </criteriaItems>
        <description>A notification from the KYC object when it reaches &quot;KYC Complete&quot;</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>GE_PRM_Notify_Channel_Manager_On_NeedMoreInfo</fullName>
        <actions>
            <name>GE_PRM_Notify_Channel_Manager_on_NeedMoreInfo</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>GE_PRM_KYC_Termination_Checklist__c.GE_PRM_Status__c</field>
            <operation>equals</operation>
            <value>KYC Need More Information</value>
        </criteriaItems>
        <description>This workflow rule will send an email to the Channel Manager when KYC needs Additional Info.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>KYC Inbound Response Time Calculation</fullName>
        <actions>
            <name>KYC_Inbound_Response_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(GE_HQ_Status__c), OR(ISPICKVAL(GE_HQ_Status__c,&apos;Manual Due Diligence Approved&apos;),ISPICKVAL(GE_HQ_Status__c,&apos;Manual Due Diligence Rejected&apos;)) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>KYC Outbound Response Time Calculation</fullName>
        <actions>
            <name>KYC_Outbound_Response_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>OR(AND(ISCHANGED(GE_HQ_Status__c), ISPICKVAL(PRIORVALUE(GE_HQ_Status__c),&apos;Pending User Submission&apos;),ISPICKVAL(GE_HQ_Status__c,&apos;Manual Due Diligence Submitted&apos;)), AND(ISCHANGED(GE_HQ_Status__c), ISPICKVAL(PRIORVALUE(GE_HQ_Status__c),&apos;Pending User Submission&apos;),ISPICKVAL(GE_HQ_Status__c,&apos;Manual Due Diligence Approved&apos;)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>KYC SentBack%2FSubmitted Cycle Time</fullName>
        <actions>
            <name>GE_OG_Update_Final_KYC_Sent_Back</name>
