    <rules>
        <fullName>Sales_eRAD_is_Created</fullName>
        <actions>
            <name>Email_to_Sales_when_eRAD_Created</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>If(RecordType.Name == &apos;Subsea&apos;,AND(ISPICKVAL(Status__c,&apos;Sales Mode&apos;)  ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 20000000.00),IF(OR(RecordType.Name == &apos;TMS - CSA&apos;,RecordType.Name == &apos;TMS New Units/Opex&apos;),AND(ISPICKVAL(Status__c,&apos;Sales Mode&apos;)  ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 5000000.00),null))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>eRAD_remains_in_ComOps_Mode</fullName>
        <active>true</active>
        <formula>If(RecordType.Name == &apos;Subsea&apos;,AND(ISPICKVAL(Status__c,&apos;ComOps Mode&apos;) ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 20000000.00),IF(OR(RecordType.Name == &apos;TMS - CSA&apos;,RecordType.Name == &apos;TMS New Units/Opex&apos;),AND(ISPICKVAL(Status__c,&apos;ComOps Mode&apos;) ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 5000000.00),null))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Approver_approves_Sales_eRAD_to_ComOps</name>
                <type>Alert</type>
            </actions>
            <timeLength>5</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Approver_approves_Sales_eRAD_to_ComOps</name>
                <type>Alert</type>
            </actions>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>eRAD_remains_in_Sales_Mode</fullName>
        <active>true</active>
        <formula>If(RecordType.Name == &apos;Subsea&apos;,AND(ISPICKVAL(Status__c,&apos;Sales Mode&apos;) ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 20000000.00),IF(OR(RecordType.Name == &apos;TMS - CSA&apos;,RecordType.Name == &apos;TMS New Units/Opex&apos;),AND(ISPICKVAL(Status__c,&apos;Sales Mode&apos;) ,ISPICKVAL(Approval_Status__c,&apos;Draft&apos;),Opportunity_Amount__c &gt;= 5000000.00),null))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Email_to_Sales_when_eRAD_Created</name>
                <type>Alert</type>
            </actions>
            <timeLength>20</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Email_to_Sales_when_eRAD_Created</name>
                <type>Alert</type>
            </actions>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>eRad_SS_Draft_Mode</fullName>
        <actions>
            <name>eRad_SS_Draft_Mode</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>eRAD__c.RecordTypeId</field>
            <operation>contains</operation>
            <value>Subsea,Subsea Read Only</value>
        </criteriaItems>
        <criteriaItems>
            <field>eRAD__c.Approval_Status__c</field>
            <operation>contains</operation>
            <value>Draft,Approved</value>
