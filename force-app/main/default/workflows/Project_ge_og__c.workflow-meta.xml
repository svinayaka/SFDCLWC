        <actions>
            <name>Sending_Email_Alert_To_CPR_ActMnger_and_ProjectOwner</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Project_ge_og__c.GE_OG_TMS_EventsModified__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Project_ge_og__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Site Outage</value>
        </criteriaItems>
        <criteriaItems>
            <field>Project_ge_og__c.Name</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
