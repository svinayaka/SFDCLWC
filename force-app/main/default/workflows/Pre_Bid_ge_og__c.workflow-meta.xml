        </criteriaItems>
        <criteriaItems>
            <field>Pre_Bid_ge_og__c.pre_bid_active_ge_og__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
