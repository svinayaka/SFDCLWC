    <rules>
        <fullName>Pre-Bid Evaluation to false</fullName>
        <actions>
            <name>Pre_Bid_Evaluation_to_false</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(NOT(ISPICKVAL(pre_bid_status_ge_og__c, &apos;Submitted&apos;)) , NOT(ISPICKVAL(pre_bid_status_ge_og__c, &apos;Approved&apos;) ), pre_bid_active_ge_og__c == true)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Pre-Bid Evaluation to true</fullName>
        <actions>
            <name>Pre_Bid_Evaluation_On_Oppty_to_true</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2) AND 3</booleanFilter>
        <criteriaItems>
            <field>Pre_Bid_ge_og__c.pre_bid_status_ge_og__c</field>
            <operation>equals</operation>
            <value>Submitted</value>
        </criteriaItems>
        <criteriaItems>
            <field>Pre_Bid_ge_og__c.pre_bid_status_ge_og__c</field>
