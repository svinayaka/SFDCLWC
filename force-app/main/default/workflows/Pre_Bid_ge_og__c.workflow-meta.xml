    <rules>
        <fullName>Bid No Bid Approval to false</fullName>
        <actions>
            <name>Update_Bid_No_BId_Approval_to_false</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Pre_Bid_ge_og__c.pre_bid_status_ge_og__c</field>
            <operation>notEqual</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Pre_Bid_ge_og__c.pre_bid_active_ge_og__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.tier_2_ge_og__c</field>
            <operation>equals</operation>
            <value>Oil Field Services</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
