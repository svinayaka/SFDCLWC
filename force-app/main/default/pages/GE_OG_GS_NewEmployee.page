<apex:page standardController="SVMXC__Service_Group_Members__c" extensions="GE_OG_GS_NewEmployeeExtn">
     <apex:form >
         <apex:sectionheader title="Employee Edit" subtitle="{!if(SVMXC__Service_Group_Members__c.Id==null,'New Employee',SVMXC__Service_Group_Members__c.Name)}"></apex:sectionheader>
         <apex:pageblock mode="edit"  title="Employee Edit">
         <apex:pageblockbuttons >
             <apex:commandbutton action="{!save}" value="Save"></apex:commandbutton>
             <apex:commandbutton action="{!cancel}" value="Cancel"></apex:commandbutton>
         </apex:pageblockbuttons> 
         <apex:pageMessages id="errormessages"></apex:pageMessages><!-- displays all message generated by the component -->
             <apex:pageblocksection id="info" title="Employee Information" columns="1">
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_SM_HQ_SSO_ID__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.SVMXC__Service_Group__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_GS_FSE_Type__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_MC_Nationality__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_GS_Resident__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_GS_Site_Manager__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_GS_Fit_to_Work_Date__c}" required="true"/>
                 <apex:inputField value="{!SVMXC__Service_Group_Members__c.GE_OG_GS_Fit_to_Work_expiry_Date__c}" required="true"/>

             </apex:pageblocksection>
         </apex:pageblock>
     </apex:form>
 </apex:page>