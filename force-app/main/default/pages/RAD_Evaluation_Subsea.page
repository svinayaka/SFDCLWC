                <apex:pageBlock >
                    <apex:image url="{!$Resource.eRAD_Rating_Scale}" width="670" height="230"/>
                </apex:pageBlock>
            </apex:tab>
            
            <apex:tab label="Final Statement" labelWidth="140%" reRender="frm" name="FinalStatement" id="FS">
                <apex:pageBlock >
                    <apex:outputPanel rendered="{!errormsg}">
                    <apex:pageMessages /><br/>
                    </apex:outputPanel>
                    <apex:outputtext value="{!$ObjectType.eRAD__c.fields.Highllights__c.label}" style="font-weight:bold;font-size:15px"/><br/>
                    <apex:inputField value="{!eRadRecRes[0].Highllights__c}" label="" style="width:100%;height:100px;"/><br/><br/>
                    <div align="center">
                        <apex:commandButton value="Submit" action="{!savecontent}" onclick="calallrating();" styleClass="button" rendered="{!buttonboo == true}"/>&nbsp;&nbsp;
                        <apex:commandButton value="Save" onclick="calallrating();" action="{!save_custom}" styleClass="button" rendered="{!buttonboo == true}"/>
                        <apex:outputpanel >&nbsp;<apex:commandButton value="Cancel" action="{!cancelpage}" styleClass="button" /></apex:outputpanel>
                        <apex:outputpanel rendered="{!buttonboo == true}">&nbsp;<apex:commandButton value="Report" rendered="{!buttonboo == true}" onclick="calallratingreport();" rerender="pageid" styleClass="button" /></apex:outputpanel>
                        <apex:outputpanel rendered="{!buttonboo == false}">&nbsp;<apex:commandButton value="Report" rendered="{!buttonboo == false}" onclick="window.open('/apex/eRAD_Custom_Report?id={!eradid}','_blank');" rerender="pageid" styleClass="button" /></apex:outputpanel>
                    </div>
                </apex:pageBlock>
            </apex:tab>
        </apex:tabPanel>
    </apex:form>
</apex:page>