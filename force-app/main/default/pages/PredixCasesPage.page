    <script src="../../soap/ajax/34.0/connection.js" type="text/javascript"></script>
    <script type="text/javascript">    
    var writtenHTML = '';
   
    
    
    function setupPage() {
      //function contains all code to execute after page is rendered
      var searchKey = $('#idglobalsearch').val();
      console.log(searchKey);
      if(searchKey =='')
      {
         $('#idULsuggestedcases').html('');
         $('#idULsuggestedcases').Hide(); 
          return;
      }
      writtenHTML = '<ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content" id="ui-id-1" tabindex="0" style="display: block; position: relative; top: 0px; left: 1px; width: 1258px;"><li class="ui-menu-category">cases</li>';
     
      var resultCases =  sforce.connection.query("Select Id, Subject,CaseNumber,RecordType.Name From Case where  (Subject like '"+searchKey+"%' and Subject !=null) OR (CaseNumber like '"+searchKey+"%' and CaseNumber !=null) limit 10");
      //console.log(resultCases.size);
      var resultKnowledge = sforce.connection.query(
          "SELECT Summary,Title,UrlName, Language, PublishStatus, KnowledgeArticleId FROM KnowledgeArticleVersion where Title like '"+searchKey+"%' and PublishStatus ='Online' and Language='en_US' and Title !=null limit 10");
         
    if (resultCases.size > 0) {
      //get the records array
      var records = resultCases.getArray('records');
      //loop through the records and construct html string
      for (var i = 0; i < records.length; i++) {
        var cases = records[i];
       
       writtenHTML +=  '<li aria-label="Case : '+cases.CaseNumber +':'+ cases.Subject +'" class="ui-menu-item" id="ui-id-'+cases.Id+'" tabindex="-1"><a href="#top">' +cases.CaseNumber +':'+ cases.Subject+'</a></li>';
      }
    
    }
    else{
        writtenHTML +=  '<li aria-label="Case : No data" class="ui-menu-item" id="ui-id-255" tabindex="-1"><a href="#top">No data</a>  </li>';
    }
    
    //console.log(resultKnowledge.size);
    writtenHTML +='<li class="ui-menu-category">knowledge</li>';
    if(resultKnowledge.size > 0){
       var records = resultKnowledge.getArray('records');
       
 
      //loop through the records and construct html string
      for (var i = 0; i < records.length; i++) {
        var knowledge = records[i];
 
        writtenHTML +=  '<li aria-label="Knowledge : '+knowledge.Summary +'" class="ui-menu-item" id="ui-id-255" tabindex="-1"><a href="#top">' + knowledge.Summary+'</a> </li>';
       
      }
      
      }
      else{
         writtenHTML +=  '<li aria-label="Knowledge : No data" class="ui-menu-item" id="ui-id-255" tabindex="-1"><a href="#top">No data</a> </li>';
      }
      if(resultCases.size > 0 || resultKnowledge.size > 0){
            writtenHTML+='</ul>';  
            $('#idULsuggestedcases').html(writtenHTML);  
            $('#idULsuggestedcases').Show();  
      }
     /* else
      {
        $('#idULsuggestedcases').html('');   
      }
      */
     
    //render the generated html string
   
    
  }
 
  function queryFailed(error, source) {
    source.output.innerHTML = "An error has occurred: " + error;
  }
 
  </script>
